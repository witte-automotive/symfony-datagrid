{
    "name": "witte/sydatagrid",
    "description": "Symfony DataGrid",
    "type": "symfony-bundle",
    "license": "proprietary",
     "authors": [
        {
            "name": "Oleg Kuzmenko",
            "email": "oleg.kuzmenko@witte-automotive.cz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "doctrine/orm": "^2.0 || ^3.0",
        "symfony/asset": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/framework-bundle": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/twig-bundle": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/ux-icons": "^2.25",
        "symfony/ux-twig-component": "^2.0",
        "symfony/yaml": "^4.4 || ^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "SyDataGrid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SyDataGrid\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4 || ^5.0 || ^6.0 || ^7.0"
        }
    },
    "config": {
    }
}
